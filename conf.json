{
        "plugins": [],
        "recurseDepth": 10,
        "source": {
            "includePattern": ".+\\.js(doc|x)?$",
            "excludePattern": "(^|\\/|\\\\)_"
        },
        "sourceType": "module",
        "tags": {
            "allowUnknownTags": true,
            "dictionaries": [
                "jsdoc",
                "closure"
            ]
        },
        "templates": {
            "default": {
                "outputSourceFiles": true
            },
            "cleverLinks": false,
            "monospaceLinks": false
        },
        "opts": {
            "destination": "./docs/",
            "package": "./package.json",
            "recurse": true,
            "lenient": true,
            "template": "node_modules/clean-jsdoc-theme"            
        }
    
}